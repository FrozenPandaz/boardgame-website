{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "boardgame-website": {
      "root": "apps/boardgame-website/",
      "sourceRoot": "apps/boardgame-website/src",
      "projectType": "application",
      "prefix": "boardgame-website",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/boardgame-website",
            "index": "apps/boardgame-website/src/index.html",
            "main": "apps/boardgame-website/src/main.ts",
            "polyfills": "apps/boardgame-website/src/polyfills.ts",
            "tsConfig": "apps/boardgame-website/tsconfig.app.json",
            "assets": [
              "apps/boardgame-website/src/favicon.ico",
              "apps/boardgame-website/src/assets"
            ],
            "styles": [
              "apps/boardgame-website/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/boardgame-website/src/environments/environment.ts",
                  "with": "apps/boardgame-website/src/environments/environment.prod.ts"
                }
              ],
              "deployUrl": "/boardgame-website/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "boardgame-website:build",
            "proxyConfig": "apps/boardgame-website/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "boardgame-website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "boardgame-website:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/boardgame-website/tsconfig.app.json",
              "apps/boardgame-website/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/boardgame-website/jest.config.js",
            "tsConfig": "apps/boardgame-website/tsconfig.spec.json",
            "setupFile": "apps/boardgame-website/src/test-setup.ts"
          }
        }
      }
    },
    "boardgame-website-e2e": {
      "root": "apps/boardgame-website-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/boardgame-website-e2e/cypress.json",
            "tsConfig": "apps/boardgame-website-e2e/tsconfig.e2e.json",
            "devServerTarget": "boardgame-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "boardgame-website:serve:production"
            },
            "ci": {
              "devServerTarget": "boardgame-website:serve",
              "headless": true
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/boardgame-website-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "home": {
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "projectType": "library",
      "prefix": "boardgame-website",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/home/tsconfig.lib.json",
              "libs/home/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "tsConfig": "libs/home/tsconfig.spec.json",
            "setupFile": "libs/home/src/test-setup.ts"
          }
        }
      }
    },
    "theme": {
      "root": "libs/theme",
      "sourceRoot": "libs/theme/src",
      "projectType": "library",
      "prefix": "boardgame-website",
      "architect": {}
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/builders:node-build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/builders:node-execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "boardgame-data-access": {
      "root": "libs/boardgame/data-access",
      "sourceRoot": "libs/boardgame/data-access/src",
      "projectType": "library",
      "prefix": "boardgame-website",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/boardgame/data-access/tsconfig.lib.json",
              "libs/boardgame/data-access/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/boardgame/data-access/jest.config.js",
            "tsConfig": "libs/boardgame/data-access/tsconfig.spec.json"
          }
        }
      }
    },
    "boardgame-contract": {
      "root": "libs/boardgame/contract",
      "sourceRoot": "libs/boardgame/contract/src",
      "projectType": "library",
      "prefix": "boardgame-website",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/boardgame/contract/tsconfig.lib.json",
              "libs/boardgame/contract/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/boardgame/contract/jest.config.js",
            "tsConfig": "libs/boardgame/contract/tsconfig.spec.json"
          }
        }
      }
    },
    "fetch-xml": {
      "root": "libs/fetch-xml",
      "sourceRoot": "libs/fetch-xml/src",
      "projectType": "library",
      "prefix": "boardgame-website",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/fetch-xml/tsconfig.lib.json",
              "libs/fetch-xml/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/fetch-xml/jest.config.js",
            "tsConfig": "libs/fetch-xml/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "defaultProject": "boardgame-website"
}
